<h1>アナグラさんとのチャット</h1>

<h2><%= link_to 'ペアを追加する', new_user_path %></h2>

<table>
  <tr>
    <th>Name</th>
    <th>password</th>
    <th></th>
  </tr>

  <% @users.each do |article| %>
    <tr>
      <td><%= article.name %></td>
      <td><%= article.password %></td>
      <td><%= link_to 'Show', user_path(article) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'ホームに戻る', controller: 'users'%>
